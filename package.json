{
	"name": "decorative-navigation",
	"displayName": "Decorative Navigation",
	"description": "Navigate files by decorated symbols",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:decorative-navigation.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "decorative-navigation.helloWorld",
				"title": "Hello World"
			}
		],
		"colors": [
			{
				"id": "decorativeNavigation.green",
				"description": "Decoration color to use for green symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.red",
				"description": "Decoration color to use for red symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.default",
				"description": "Decoration color to use for default symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.green",
				"description": "Decoration color to use for green symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.red",
				"description": "Decoration color to use for red symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.gray",
				"description": "Decoration color to use for gray symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.brown",
				"description": "Decoration color to use for brown symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.teal",
				"description": "Decoration color to use for teal symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.mauve",
				"description": "Decoration color to use for mauve symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			},
			{
				"id": "decorativeNavigation.blue",
				"description": "Decoration color to use for blue symbols",
				"defaults": {
					"dark": "#00FF00",
					"light": "#00FF00",
					"highContrast": "#00FF00"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
