{
	"name": "decorative-navigation",
	"displayName": "Decorative Navigation",
	"description": "Navigate files by decorated symbols",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "decorative-navigation.selectToken",
				"title": "Decorative navigation: Select token"
			}
		],
		"colors": [
			{
				"id": "decorativeNavigation.defaultBorder",
				"description": "Border color to use for default symbols",
				"defaults": {
					"dark": "#b9b4da",
					"light": "#b9b4da",
					"highContrast": "#b9b4da"
				}
			},
			{
				"id": "decorativeNavigation.greenBorder",
				"description": "Border color to use for green symbols",
				"defaults": {
					"dark": "#13940d",
					"light": "#13940d",
					"highContrast": "#13940d"
				}
			},
			{
				"id": "decorativeNavigation.redBorder",
				"description": "Border color to use for red symbols",
				"defaults": {
					"dark": "#a03b00",
					"light": "#a03b00",
					"highContrast": "#a03b00"
				}
			},
			{
				"id": "decorativeNavigation.yellowBorder",
				"description": "Border color to use for yellow symbols",
				"defaults": {
					"dark": "#c5a800",
					"light": "#c5a800",
					"highContrast": "#c5a800"
				}
			},
			{
				"id": "decorativeNavigation.mauveBorder",
				"description": "Border color to use for mauve symbols",
				"defaults": {
					"dark": "#9100f1",
					"light": "#9100f1",
					"highContrast": "#9100f1"
				}
			},
			{
				"id": "decorativeNavigation.blueBorder",
				"description": "Border color to use for blue symbols",
				"defaults": {
					"dark": "#00a0df",
					"light": "#00a0df",
					"highContrast": "#00a0df"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@types/lodash": "^4.14.168",
		"lodash": "^4.17.20"
	}
}