{
	"name": "decorative-navigation",
	"displayName": "Decorative Navigation",
	"description": "Navigate files by decorated symbols",
	"version": "0.1.0",
	"publisher": "pokey",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pokey/decorative-navigation.git"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "decorative-navigation.selectToken",
				"title": "Decorative navigation: Select token"
			},
			{
				"command": "decorative-navigation.toggleDecorations",
				"title": "Decorative navigation: Toggle decorations"
			}
		],
		"colors": [
			{
				"id": "decorativeNavigation.defaultBorder",
				"description": "Border color to use for default symbols",
				"defaults": {
					"dark": "#cdc7f0",
					"light": "#000000",
					"highContrast": "#000000"
				}
			},
			{
				"id": "decorativeNavigation.greenBorder",
				"description": "Border color to use for green symbols",
				"defaults": {
					"dark": "#22d618",
					"light": "#2ec426",
					"highContrast": "#2ec426"
				}
			},
			{
				"id": "decorativeNavigation.redBorder",
				"description": "Border color to use for red symbols",
				"defaults": {
					"dark": "#e96900",
					"light": "#ff3300",
					"highContrast": "#ff3300"
				}
			},
			{
				"id": "decorativeNavigation.yellowBorder",
				"description": "Border color to use for yellow symbols",
				"defaults": {
					"dark": "#f1cd00",
					"light": "#f0b800",
					"highContrast": "#f0b800"
				}
			},
			{
				"id": "decorativeNavigation.mauveBorder",
				"description": "Border color to use for mauve symbols",
				"defaults": {
					"dark": "#de25ff",
					"light": "#ff00f2",
					"highContrast": "#ff00f2"
				}
			},
			{
				"id": "decorativeNavigation.blueBorder",
				"description": "Border color to use for blue symbols",
				"defaults": {
					"dark": "#00b7ff",
					"light": "#19a1d6",
					"highContrast": "#19a1d6"
				}
			}
		],
		"configuration": {
			"title": "Decorative navigation",
			"properties": {
				"decorative-navigation.showOnStart": {
					"type": "boolean",
					"default": true,
					"description": "Whether to show decorations on vscode start."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@types/lodash": "^4.14.168",
		"lodash": "^4.17.20"
	}
}